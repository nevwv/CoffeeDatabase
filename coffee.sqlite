CREATE TABLE coffee (
    id INTEGER PRIMARY KEY,
    name TEXT NOT NULL,
    roast_level TEXT NOT NULL,
    grind TEXT NOT NULL,
    taste_description TEXT NOT NULL,
    price REAL NOT NULL,
    package_volume REAL NOT NULL
);

